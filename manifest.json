{
	"name":"Mondirandom",
	"description":"Display a famous painting by Piet Mondirandom (1872 - 1944) on each newly opened tab",
	"version":"0.1",
	"chrome_url_overrides": {
	   "newtab": "newtab.html"
	},
	"icons": { "16": "icon_16.png",
	           "48": "icon_48.png",
	          "128": "icon_128.png" },
	"content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self';",
	"manifest_version":2
}